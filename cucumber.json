{
"default": {
    "formatOptions": {
        "snippetInterface": "async-await"
    },
    "paths": [
        "src/test/features/*.feature"
        ],
        "format": [
            "html:reports/cucumber-html-report.html"
        ],
        "parallel": 1,
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "steps/**/*.ts", "support/World.ts", "support/env.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ]
    }
}